/**app.wxss**/
page {
  --box-tile-color: #54E97C;
  --button-background-color: #D9D9D9;
  --box-background-color: #F2F2F2;
  --item-background-color: #D9D9D9;
  --card-background-color: #D9D9D9;
  --son-item-background-color: #F1F1F1;
}

.big-button {
  padding: 8px 15px 8px 15px;
  font-size: 20px;
  font-weight: 600;
  background-color: var(--button-background-color);
  border-radius: 10px;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
